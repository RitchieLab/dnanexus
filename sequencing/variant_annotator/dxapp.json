{
  "name": "variant_annotator",
  "title": "GATK VariantAnnotator",
  "summary": "Annotate a VCF file using GATK VariantAnnotator",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "categories": [
    "Variation Calling"
  ],
  "inputSpec": [
    {
      "name": "variants_vcfgzs",
      "label": "VCF File",
      "class": "array:file",
      "optional": false,
      "patterns": [
        "*.vcf.gz"
      ]
    },
    {
      "name": "variants_vcfgztbis",
      "label": "VCF Index File",
      "class": "array:file",
      "optional": false,
      "patterns": [
        "*.tbi"
      ]
    },
    {
      "name": "gatk_jar",
      "label": "GATK3 jar file",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.jar"
      ]
    },
    {
      "name": "genome_fastagz",
      "label": "Reference genome",
      "class": "file",
      "optional": false,
      "suggestions": [
        {
          "name": "DNAnexus Reference Genomes: AWS US-east",
          "project": "project-BQpp3Y804Y0xbyG4GJPQ01xv",
          "path": "/"
        },
        {
          "name": "DNAnexus Reference Genomes: AWS China-north",
          "project": "project-Bxv84pQ27z999kj0jbQz0FK9",
          "path": "/"
        },
        {
          "name": "DNAnexus Reference Genomes: AWS Germany",
          "project": "project-F3zxk7Q4F30Xp8fG69K1Vppj",
          "path": "/"
        },
        {
          "name": "DNAnexus Reference Genomes: Azure West US",
          "project": "project-F0yyz6j9Jz8YpxQV8B8Kk7Zy",
          "path": "/"
        },
        {
          "name": "DNAnexus Reference Genomes: AWS Sydney",
          "project": "project-F4gXb605fKQyBq5vJBG31KGG",
          "path": "/"
        }
      ]
    },
    {
      "name" : "no_geno",
      "label" : "Suppress all samples VCF",
      "class" : "boolean",
      "optional" : false,
      "default" : false
    },
    {
      "name": "hdr_only",
      "label": "Output Sites-only VCF",
      "class": "boolean",
      "optional": false,
      "default" : true
    },
    {
      "name": "cmd_params",
      "label": "VariantAnnotator Parameters",
      "class": "string",
      "optional": true
    }
  ],
  "outputSpec": [
    {
      "name": "vcf_out",
      "label": "Filtered VCF file",
      "class": "array:file",
      "optional" : true,
      "patterns": [
        "*.vcf.gz"
      ]
    },
    {
      "name": "vcfidx_out",
      "label": "Filtered VCF Index file",
      "class": "array:file",
      "optional" : true,
      "patterns": [
        "*.tbi"
      ]
    },
    {
      "name": "vcf_hdr_out",
      "label": "Filtered VCF Sites-only file",
      "class": "array:file",
      "optional" : true,
      "patterns": [
        "*.vcf.gz"
      ]
    },
    {
      "name": "vcfidx_hdr_out",
      "label": "Filtered VCF Sites-only Index file",
      "class": "array:file",
      "optional" : true,
      "patterns": [
        "*.tbi"
      ]
    }
  ],
  "runSpec": {
    "distribution": "Ubuntu",
    "release": "14.04",
    "interpreter": "python2.7",
    "file": "src/variant_annotator.py"
  },
  "regionalOptions": {
    "aws:us-east-1": {"systemRequirements": {"*": {"instanceType": "mem1_ssd1_x8"}},
                      "assetDepends": [{"id": "record-FGkBxFj0yGxqfpJJ25JqbybV"}]}
  },
  "developers": ["user-bhannigan"],
  "openSource" : true
}
